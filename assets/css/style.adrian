.ui-state-default {
    color: #454545 !important;
}

.radio-btn:checked,
.radio-btn:not(:checked) {
    position: absolute;
    left: -9999px;
}

.radio-btn:checked+label,
.radio-btn:not(:checked)+label {
    position: relative;
    padding-left: 28px;
    cursor: pointer;
    line-height: 20px;
    display: inline-block;
    color: #000;
}

.modal-salon-subpage .radio-btn:checked+label,
.modal-salon-subpage .radio-btn:not(:checked)+label {
    color: #fff !important;
}

.radio-btn:checked+label:before,
.radio-btn:not(:checked)+label:before {
    content: '';
    position: absolute;
    left: 0;
    top: 0;
    width: 18px;
    height: 18px;
    border: 1px solid #888;
    border-radius: 100%;
    background: #fff;
}

.radio-btn:checked+label:after,
.radio-btn:not(:checked)+label:after {
    content: '';
    width: 12px;
    height: 12px;
    background: #000;
    position: absolute;
    top: 3px;
    left: 3px;
    border-radius: 100%;
    -webkit-transition: all 0.2s ease;
    transition: all 0.2s ease;
}

.radio-btn:not(:checked)+label:after {
    opacity: 0;
    -webkit-transform: scale(0);
    transform: scale(0);
}

.radio-btn:checked+label:after {
    opacity: 1;
    -webkit-transform: scale(1);
    transform: scale(1);
}

.checkbox-btn:checked,
.checkbox-btn:not(:checked) {
    position: absolute;
    left: -9999px;
}

.checkbox-btn:checked+label,
.checkbox-btn:not(:checked)+label {
    position: relative;
    padding-left: 28px;
    cursor: pointer;
    line-height: 20px;
    display: inline-block;
    color: #000;
}

.checkbox-btn:checked+label:before,
.checkbox-btn:not(:checked)+label:before {
    content: '';
    position: absolute;
    left: 0;
    top: 0;
    width: 20px;
    height: 20px;
    border: 1px solid #888;
    background: #fff;
}

.checkbox-btn:checked+label:after,
.checkbox-btn:not(:checked)+label:after {
    content: '\2713';
    width: 20px;
    height: 20px;
    position: absolute;
    font-weight: bold;
    top: 0px;
    left: 4px;
    border-radius: 100%;
    -webkit-transition: all 0.2s ease;
    transition: all 0.2s ease;
}

.checkbox-btn:not(:checked)+label:after {
    opacity: 0;
    -webkit-transform: scale(0);
    transform: scale(0);
}

.checkbox-btn:checked+label:after {
    opacity: 1;
    -webkit-transform: scale(1);
    transform: scale(1);
}

