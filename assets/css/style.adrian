button.btn-next:after {
    content: '»';
    position: absolute;
    opacity: 0;
    top: 5px;
    right: -20px;
    transition: 0.5s;
    font-size: 1em;
}

button.btn-next:hover {
    padding-right: 50px;
    padding-left: 50px;
}

button.btn-next:hover:after {
    opacity: 1;
    right: 10px;
}

button.btn-previous:after {
    content: '«';
    position: absolute;
    opacity: 0;
    top: 5px;
    left: -20px;
    transition: 0.5s;
    font-size: 1em;
}

button.btn-previous:hover {
    padding-right: 50px;
    padding-left: 50px;
}

button.btn-previous:hover:after {
